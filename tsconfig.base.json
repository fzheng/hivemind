{
  "compilerOptions": {
    "target": "ES2020",
    "module": "node16",
    "moduleResolution": "node16",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "lib": ["ES2020", "DOM"],
    "sourceMap": true,
    "noImplicitAny": true,
    "isolatedModules": true,
    "baseUrl": ".",
    "paths": {}
  }
}
